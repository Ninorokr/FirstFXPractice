<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.silverlink.firstfxpractice.ComboBoxController">
   <children>
      <ComboBox fx:id="myComboBox" layoutX="222.0" layoutY="249.0" onAction="#comboChanged" prefWidth="150.0" promptText="Select a fruit">
         <items>
            <FXCollections fx:factory="observableArrayList">
               <String fx:value="First Item"/>
               <String fx:value="Second Item"/>
               <String fx:value="Third Item"/>
               <String fx:value="Fourth Item"/>
               <String fx:value="Fifth Item"/>
               <String fx:value="Sixth Item"/>
            </FXCollections>
         </items>
      </ComboBox>
      <Label fx:id="myLabel" layoutX="225.0" layoutY="89.0" prefHeight="45.0" prefWidth="143.0" />
      <Button fx:id="btnAddItems" layoutX="210.0" layoutY="295.0" mnemonicParsing="false" onAction="#ButtonCombo" prefWidth="174.0" text="Add items" />
   </children>
</AnchorPane>
